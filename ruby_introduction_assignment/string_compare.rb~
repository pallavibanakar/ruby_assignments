begin
def compare(string_one,string_two)
   
     return string_one===string_two
     
end


   puts "enter the string to be compared"
   string_one=gets.chomp
   puts "enter the string to be compared with "
   string_two=gets.chomp
   check=compare(string_one,string_two)
   puts check
   if check then
      puts "yes its a match"
   else
      puts "no,its not a match"
   end
end
   

  
